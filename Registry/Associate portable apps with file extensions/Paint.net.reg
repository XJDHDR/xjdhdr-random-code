; Assign file types to be opened using Paint.net.
[HKEY_CLASSES_ROOT\.dds]
@="Paint.NET.Picture"

[-HKEY_CLASSES_ROOT\.dds\ShellNew]

[-HKEY_CURRENT_USER\SOFTWARE\Microsoft\Windows\Roaming\OpenWith\FileExts\.dds]
[-HKEY_CURRENT_USER\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\FileExts\.dds]


[-HKEY_CLASSES_ROOT\.pdn]
[HKEY_CLASSES_ROOT\.pdn]
@="Paint.NET.Picture"
"Content Type"="image/x-paintnet"
"PerceivedType"="image"

[HKEY_CLASSES_ROOT\.pdn\PersistentHandler]
